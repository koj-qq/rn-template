{
  "name": "rnTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "codePush": "node scripts/codePush/index.js",
    "clean": "cd ./android && ./gradlew clean",
    "android:release": "npm run clean && npm run jetifier && cd ./android && ./gradlew app:assembleRelease",
    "android:staging": "npm run clean && npm run jetifier && cd ./android && ./gradlew app:assembleReleaseStaging",
    "tsc": "tsc -p ./tsconfig.json",
    "eslint": "eslint --ext .ts,.tsx src/",
    "eslint:fix": "eslint --fix --ext .ts,.tsx src/",
    "lint": "prettier --check src/**/*.ts src/**/*.tsx",
    "lint:fix": "prettier --check src/**/*.tsx --write",
    "commit": "git-cz",
    "jetifier": "npx jetifier",
    "pod": "cd ios && pod install && cd ..",
    "test": "jest --watch"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run eslint:fix && npm run lint:fix && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "git add ."
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.11.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@td-design/react-native": "^1.3.4",
    "ahooks": "^2.1.0",
    "dayjs": "^1.10.2",
    "immer": "^8.0.0",
    "lodash-es": "^4.17.15",
    "react": "^17.0.1",
    "react-native": "0.63.4",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-vector-icons": "^7.1.0",
    "rn-fetch-blob": "^0.12.0",
    "umi-request": "^1.3.5",
    "use-immer": "^0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-flow-strip-types": "^7.10.1",
    "@babel/runtime": "^7.10.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@shopify/restyle": "^1.4.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-hooks": "^3.7.0",
    "@testing-library/react-native": "^7.1.0",
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.19",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.43",
    "@types/react-native-vector-icons": "^6.4.6",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-jest": "^26.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.1.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "husky": "^4.2.5",
    "inquirer": "^7.3.3",
    "jest": "^26.1.0",
    "jetifier": "^1.6.5",
    "lint-staged": "^10.2.9",
    "metro-react-native-babel-preset": "^0.64.0",
    "pont-engine": "1.0.10",
    "prettier": "^2.0.5",
    "react-test-renderer": "^17.0.1",
    "shelljs": "^0.8.4",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  }
}
